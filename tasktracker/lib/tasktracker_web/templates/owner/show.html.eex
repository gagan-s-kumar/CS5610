<h2>Show Owner</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @owner.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @owner.name %>
  </li>

</ul>

<span><%= link "Edit", to: owner_path(@conn, :edit, @owner) %></span>
<span><%= link "Back", to: owner_path(@conn, :index) %></span>
